{
    "name": "Simple PHP MariaDB Project",
    "services": [
        {
            "type": "nginx",
            "path": "./app",
            "host": "localhost"
        },
        {
            "type": "php",
            "path": "./app",
            "host": "localhost",
            "options": {
                "version": "7.1"
            }
        },
        {
            "type": "mariadb",
            "options": {
                "version": "10.3"
            }
        }
    ]
}
